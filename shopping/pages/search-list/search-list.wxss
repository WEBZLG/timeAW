/* shopping/pages/search-list/search-list.wxss */
/* 暂无数据 */
.empty {
  margin-top: 50%;
  box-sizing: border-box;
  padding: 30rpx 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.empty-img {
  height: 302rpx;
  width: 489rpx;
}

.flex {
  display: flex;
  align-items: center;
}

.flex-bet {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.goods-item {
  border-bottom: 1px solid #eeeeee;
  padding: 20rpx 24rpx;
}

.goods-text {
  flex: 1;
}

.check-box {
  margin-right: 18rpx;
}

.goods-item .van-card {
  background-color: #ffffff;
}

.goods-item .van-card__thumb {
  width: 200rpx !important;
  height: 200rpx !important;
  border-radius: 10rpx;
  overflow: hidden;
}

.goods-item .van-card__title {
  font-size: 28rpx;
  color: #1a1a1a;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  height: 68rpx;
  padding-top: 10rpx;
}

.goods-item .van-card__desc {
  font-size: 24rpx;
  color: #bfbfbf;
  height: 86rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.goods-item .van-card__price {
  color: #33bcc6;
  font-size: 28rpx;
}

.price {
  font-size: 28rpx;
  color: #33bcc6;
  font-weight: bold;
}
.bar-class{
  padding: 20rpx 24rpx;
}
.price-stype{
  color: #33bcc6 !important;
  font-size: 28rpx;
  font-weight: bold;
}