page {
  background: #efefef;
}

#container {
  min-height: 100vh;
}

/* 头部 */

.head {
  width: fill-available;
  padding: 84rpx 0 75rpx 0;
  background: #fff;
}

.headLogo {
  width: 285rpx;
  height: 136rpx;
  margin: 0 auto;
}

.headLogo image {
  width: 100%;
  height: 100%;
}

.set_box {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.set, .set2 {
  width: calc(100% - 40rpx);
  height: 60rpx;
  line-height: 60rpx;
  padding: 15rpx 20rpx;
  border-bottom: 1px solid #efefef;
  background: #fff;
  display: flex;
  align-items: center;
}
.set3 {
  margin-top: 20rpx;
  width: calc(100% - 40rpx);
  height: 60rpx;
  line-height: 60rpx;
  padding: 15rpx 20rpx;
  border-bottom: 1px solid #efefef;
  background: #fff;
  display: flex;
  align-items: center;
}

.set {
  margin-top: 20rpx;
}

.set_a {
  width: 30%;
  height: 60rpx;
  float: left;
  font-size: 28rpx;
  color: #2f2f2f;
}

.set_b {
  width: 70%;
  height: 60rpx;
  float: right;
  font-size: 28rpx;
  color: #8c8c8c;
  text-align: right;
}

.r {
  width: 30rpx;
  height: 30rpx;
  margin-top: 15rpx;
  float: right;
  background: url('https://cm.chineseglory.cn/images/icon31.png') no-repeat center center;
  background-size: 100%;
  margin-left: 10rpx;
}
