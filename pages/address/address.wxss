#container {
  min-height: 100vh;
}

.address_box {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.address {
  overflow: hidden;
  border-top: 15rpx solid #eee;
  padding: 30rpx 24rpx;
}

.address_a {
  width: calc(100% - 120rpx);
  padding-right: 20rpx;
  padding-left: 31rpx;
  font-size: 24rpx;
  color: #616161;
}

.address_a_a {
  width: 100%;
  height: 50rpx;
  overflow: hidden;
}

.addressName {
  width: 50%;
  line-height: 40rpx;
  height: 40rpx;
}

.addressPhone {
  line-height: 40rpx;
  height: 40rpx;
}

.addressMsg {
  width: 100%;
 line-height: 40rpx;
  display: flex;
}

.addressMsg view {
  width: 380rpx;
  height: auto;
  white-space: normal;
  word-break: break-all;
  word-wrap: break-word;
  line-height: 40rpx;
}

.changeBtn ,.deteleBtn {
  width: 100rpx;
  height: 40rpx;
  border: 2rpx solid #ff6600;
  color: #ff6600;
  font-size: 24rpx;
  border-radius: 20rpx;
  text-align: center;
  line-height: 40rpx;
}

.space120 {
  width: 100%;
  height: 120rpx;
  overflow: hidden;
}

.btn {
  position: fixed;
  bottom: 0;
  width: 645rpx;
  height: 94rpx;
  border-radius: 47rpx;
  margin: 40rpx auto;
  left: 0;
  right: 0;
}

.addressIcon {
  width: 40rpx;
  height: 50rpx;
}

.addressRight {
  width: 20rpx;
  height: 35rpx;
}

.addressIcon image, .addressRight image ,.checkIcon image {
  width: 100%;
  height: 100%;
}
.checkIcon image  {
  border-radius: 50%;
}

.addressFirst {
  width: fill-available;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 20rpx;
}

.checkIcon {
  width: 25rpx;
  height: 25rpx;
  margin-right: 15rpx;
  
}

.addressSecond {
  width: fill-available;
  border-top: 1rpx solid #f2f2f2;
  padding-top: 30rpx ;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.addressSecondIcon {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #616161;
}

.addressSecondBtn {
  display: flex;
}
.deteleBtn {
  margin-left: 25rpx;
}