#container {
  min-height: 100vh;
  box-sizing: border-box;
  padding-bottom: 140rpx;
}

/* 海报 */
.privilege-poster {
  width: 100%;
}

/* 底部 */
.privilege-footer {
  width: 650rpx;
  height: 94rpx;
  position: fixed;
  margin: 40rpx auto;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  border-radius: 47rpx;
  overflow: hidden;
  box-shadow: 0 0 40rpx 0 rgba(0, 0, 0, 0.4);
}
.privilege-footer-fixed {
  height: 100%;
  width: 100%;
}



/* 申请 */
.apply {
  font-size: 28rpx;
}
/* 申请-标题 */
.apply-title {
  margin: 20rpx;
}
/* 申请-内容 */
.apply-content {
  font-size: 28rpx;
  border-radius: 20rpx;
  width: calc(100% - 40rpx);
  margin: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
  border: 2rpx solid #F2F2F2;
}
/* 申请-提交 */
.apply-submit {
  border-radius: 47rpx;
  overflow: hidden;
  width: 650rpx;
  height: 94rpx;
  margin: 40rpx auto;
  padding: 0;
  box-shadow: 0 0 40rpx 0 rgba(0, 0, 0, 0.4);
}
.apply-submit custom-image {
  width: 100%;
  height: 100%;
}

/* 表格 */
table {
  border-radius: 10rpx;
  border: 2rpx solid #F2F2F2;
  width: 650rpx;
  padding: 10rpx;
  margin: auto;
  margin-bottom: 40rpx;
  box-sizing: border-box;
}
table td {
  padding: 10rpx;
  vertical-align: top;
}
table td:first-of-type {
  width: 180rpx;
  white-space: nowrap;
}
chosePicture {
  margin: 10rpx;
}