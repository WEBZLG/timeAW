:host {
  position: fixed;
  z-index: 9001;
}

/* 自定义提示框（申请） */
.form-apply-container {
  position: relative;
}
/* 表单-成功-背景 */
.form-apply-background {
  width: 526rpx;
}
/* 表单-成功-按钮 */
.form-apply-button {
  margin: auto;
  width: 457rpx;
  height: 104rpx;
}

/* 自定义提示框（成功） */
.form-success-container {
  position: relative;
}
/* 自定义提示框（成功）-背景 */
.form-success-background {
  width: 526rpx;
}
/* 自定义提示框（成功）-按钮 */
.form-success-button {
  margin: auto;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  width: 457rpx;
  height: 104rpx;
  padding: 0;
  background-color: transparent;
}
.form-success-button::after {
  display: none;
}
.form-success-button-image {
  width: 100%;
  height: 100%;
  display: block;
}

.userItem {
  margin: auto;
  width:100%;
  /* padding: 0 20rpx; */
  position: absolute;
  top: 95rpx;
  left: 0;
}

.userName {
  width: fill-available;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 15rpx;
  border-bottom: 2rpx solid #e1d2c6;
  font-size: 24rpx;
}
.userLeft,.userRight {
  width: 50%;
  float: left;
  text-align: center;
}
.userLeft {
  color: #868686;
}
.userRight {
  font-weight: 600;
}
.btn {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  top: 22rpx;
  right: 20rpx;
}
.btn image {
  width: 100%;
  height: 100%;
}